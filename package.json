{
  "name": "bagaars-dev-crew",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "history": "^4.7.2",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-page-layout": "^0.9.8",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "redux-first-router": "0.0.16-next",
    "redux-first-router-link": "^1.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "css": "node-sass-chokidar --include-path ./src/assets/scss --include-path ./node_modules src/assets/scss -o src/assets/css && postcss src/assets/css/*.css --use autoprefixer -d src/assets/css",
    "watch-css": "nodemon -e scss -x 'npm run css'",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/standard",
    "lint-fix": "./node_modules/.bin/standard --fix"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "node-sass-chokidar": "0.0.3",
    "nodemon": "^1.15.0",
    "postcss-cli": "^5.0.0",
    "standard": "^10.0.3"
  },
  "standard": {
    "ignore": [
      "build/*"
    ]
  }
}
