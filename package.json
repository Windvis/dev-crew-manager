{
  "name": "bagaars-dev-crew",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-form": "^3.3.0",
    "react-page-layout": "^0.9.8",
    "react-redux": "^5.0.7",
    "react-scripts": "2.1.3",
    "react-select": "^1.2.1",
    "redux": "^3.7.2",
    "redux-first-router": "0.0.16-next",
    "redux-first-router-link": "^1.4.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "node-sass-chokidar": "0.0.3",
    "nodemon": "^1.17.1",
    "postcss-cli": "^5.0.0",
    "prettier-standard": "^9.1.1",
    "standard": "^12.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "scss": "node-sass-chokidar --include-path ./src/assets/scss --include-path ./node_modules src/assets/scss -o src/assets/css && postcss src/assets/css/*.css --use autoprefixer -d src/assets/css",
    "watch-scss": "nodemon -e scss -x 'npm run scss'",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "standard",
    "lint-fix": "standard --fix",
    "pretty": "prettier-standard 'src/**/*.js'"
  },
  "standard": {
    "ignore": [
      "build/*"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
